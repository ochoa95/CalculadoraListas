public interface ADTStack<E> {
	 /**
     * vacia el stack
     * @param 
     * @return 
     */
	public void empty();
	 /**
     * verifica que este vacio el stack
     * @param 
     * @return 
     */
	public boolean isEmpty();
	 /**
     * ingresa el valor al stack
     * @param 
     * @return 
     */
	public void push(E x) ;
	
	 /**
     * egresa valor del stack
     * @param 
     * @return 
     */
	 public E pop() throws Exception;
	 /**
     * devuelve el valor del stack
     * @param 
     * @return 
     */ 
	public int size();
	 /**
     * busca un valor dentro del stack
     * @param 
     * @return 
     */
	public E peek() throws Exception;
	
}
